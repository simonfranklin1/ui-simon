{
    "version": "0.1.8",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "deploy": "npm version patch && npm publish"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "ui-simon",
    "author": "Simon Franklin",
    "module": "dist/ui-simon.esm.js",
    "size-limit": [
        {
            "path": "dist/ui-simon.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/ui-simon.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^11.1.4",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "husky": "^9.0.11",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "size-limit": "^11.1.4",
        "tsdx": "^0.14.1",
        "tslib": "^2.6.3",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@mui/material": "^5.15.19"
    }
}
